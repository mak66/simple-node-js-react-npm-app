{
  "name": "my-app",
  "version": "0.1.0",
  "jest": {
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/coverage/**",
      "!src/index.js",
      "!src/register*.js",
      "!build/*",
      "!build/static/js/*"
    ],
    "coverageThreshold": {
      "global": {
        "lines": 80,
        "branches": 90,
        "functions": 100,
        "statements": 60
      }
    },
    "coverageReporters": [
      "json",
      "text",
      "clover",
      "html"
    ]
  },
  "private": true,
  "dependencies": {
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-scripts": "1.0.14"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom --coverage",
    "eject": "react-scripts eject",
    "test-pa11y": "./node_modules/.bin/pa11y-ci",
    "lighthouse:ci": "node_modules/lighthouse/lighthouse-cli/index.js --output-path=./lighthouse/lighthouse-report.html --quiet --chrome-flags='--headless --no-sandbox' http://localhost:3000",
    "lighthouse:view": "node_modules/lighthouse/lighthouse-cli/index.js --view --output-path=./lighthouse/lighthouse-report.html --quiet --chrome-flags='--headless' http://localhost:3000",
    "lighthouse": "node_modules/lighthouse/lighthouse-cli/index.js --output-path=./lighthouse/lighthouse-report.html --quiet --chrome-flags='--headless' http://localhost:3000"
  },
  "devDependencies": {
    "lighthouse": "^4.0.0",
    "pa11y-ci": "^2.1.1"
  }
}
